{
  "name": "membership",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "bowerJs": "cd assets/javascripts && rimraf lib/bower-components &&  bower install",
    "bowerCss": "cd assets/stylesheets && rimraf bower_components &&  bower install",
    "bowerSetup": "npm run bowerJs && npm run bowerCss",
    "copyHooks": "cp  -f ../git-hooks/pre-commit ../.git/hooks/",
    "devSetup": "npm run compile --dev && npm run copyHooks",
    "compile": "echo 'compiling assets' && grunt compile",
    "compileCss": "grunt compile:css --dev",
    "compileJs": "grunt compile:js --dev",
    "watch": "grunt watch --dev",
    "test": "grunt validate && grunt karma",
    "postinstall": "npm run bowerSetup"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.0",
    "bower": "^1.5.3",
    "grunt": "^0.4.5",
    "grunt-asset-hash": "^0.1.6",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "vinyl-fs": "2.2.1",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^17.2.0",
    "grunt-karma": "~0.11.0",
    "grunt-postcss": "^0.5.1",
    "grunt-px-to-rem": "^0.4.0",
    "grunt-sass": "^1.1.0",
    "grunt-svgmin": "^2.0.1",
    "grunt-svgstore": "^0.5.0",
    "karma": "~0.12.36",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jasmine": "^0.3.8",
    "jasmine-core":"^2.4.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-requirejs": "0.2.5",
    "load-grunt-tasks": "^3.2.0",
    "mkdirp": "^0.5.1",
    "phantomjs": "~1.9.17",
    "requirejs": "2.1.22",
    "rimraf": "^2.4.3",
    "time-grunt": "^1.2.1"
  }
}
